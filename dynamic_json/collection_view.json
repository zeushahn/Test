{
  "type": "scaffold",
  "args": {
    "appBar": {
      "type": "app_bar",
      "args": {
        "title": {
          "type": "text",
          "args": {
            "text": "Flower Garden (JSON Grid)"
          }
        },
        "centerTitle": true
      }
    },
    "body": {
      "type": "grid_view",
      "args": {
        "gridDelegate": {
          "type": "sliver_grid_delegate_with_fixed_cross_axis_count",
