            "crossAxisCount": 2,
            "crossAxisSpacing": 5.0,
            "mainAxisSpacing": 5.0
          }
        },
        "children": [
          {
            "type": "gesture_detector",
            "args": {
              "onTap": "${open_detail(imageName: 'flower_01.png')}",
